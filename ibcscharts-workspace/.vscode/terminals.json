{
  "autorun": false,
  "autoKill": true,
  "terminals": [
    {
      "name": "ibcscharts",
      "cwd": "../../ibcscharts",
      "open": true,
      "onlyMultiple": true,
      "command": "npm run dev"
    },
    {
      "name": "IRChartsPowerbi",
      "cwd": "../../IRChartsPowerbi",
      "open": true,
      "onlyMultiple": true,
      "command": "npm run start"
    },
    {
      "name": "git push origin all",
      "commands": [
        "git -C ../../ibcscharts push origin --all",
        "git -C ../../IRChartsPowerbi push origin --all",
        "exit"
      ],
      "onlySingle": true,
      "open": true
    },
    {
      "name": "git pull upstream development",
      "commands": [
        "git -C ../../ibcscharts pull upstream development",
        "git -C ../../IRChartsPowerbi pull upstream development",
        "exit"
      ],
      "onlySingle": true,
      "open": true
    },
    {
      "name": "git pull upstream master",
      "commands": [
        "git -C ../../ibcscharts pull upstream master",
        "git -C ../../IRChartsPowerbi pull upstream main",
        "exit"
      ],
      "onlySingle": true,
      "open": true
    },
    {
      "name": "git pull origin development",
      "commands": [
        "git -C ../../ibcscharts pull origin development",
        "git -C ../../IRChartsPowerbi pull origin development",
        "exit"
      ],
      "onlySingle": true,
      "open": true
    },
    {
      "name": "git pull origin master",
      "commands": [
        "git -C ../../ibcscharts pull origin master",
        "git -C ../../IRChartsPowerbi pull origin main",
        "exit"
      ],
      "onlySingle": true,
      "open": true
    },
    {
      "name": "git checkout development",
      "commands": [
        "git -C ../../ibcscharts checkout development",
        "git -C ../../IRChartsPowerbi checkout development",
        "exit"
      ],
      "onlySingle": true,
      "open": true
    },
    {
      "name": "git checkout null-suppression",
      "commands": [
        "git -C ../../ibcscharts checkout null-suppression",
        "git -C ../../IRChartsPowerbi checkout null-suppression",
        "exit"
      ],
      "onlySingle": true,
      "open": true
    },
    {
      "name": "git checkout expand-collapse",
      "commands": [
        "git -C ../../ibcscharts checkout expand-collapse",
        "git -C ../../IRChartsPowerbi checkout expand-collapse",
        "exit"
      ],
      "onlySingle": true,
      "open": true
    },
    {
      "name": "git checkout master",
      "commands": [
        "git -C ../../ibcscharts checkout master",
        "git -C ../../IRChartsPowerbi checkout main",
        "exit"
      ],
      "onlySingle": true,
      "open": true
    },
    {
      "name": "npm install",
      "commands": [
        "npm ci --prefix ../../ibcscharts -OR npm install --prefix ../../ibcscharts",
        "npm ci --prefix ../../IRChartsPowerbi -OR npm install --prefix ../../IRChartsPowerbi",
        "exit"
      ],
      "onlySingle": true,
      "open": true
    }
  ]
}
